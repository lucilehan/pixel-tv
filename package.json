{
  "name": "pixel-tv",
  "displayName": "Pixel TV",
  "description": "A retro pixel-art TV for watching YouTube inside VS Code",
  "version": "1.0.1",
  "publisher": "lucilehan",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucilehan/pixel-tv"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/icon.png",
  "activationEvents": [
    "onView:pixelTv.view",
    "onCommand:pixelTv.view.focus"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "pixelTv.view",
          "name": "Pixel TV",
          "icon": "media/icon.png",
          "contextualTitle": "Pixel TV"
        }
      ]
    },
    "commands": [],
    "keybindings": [
      {
        "command": "pixelTv.view.focus",
        "key": "ctrl+shift+y",
        "mac": "cmd+shift+y"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  }
}